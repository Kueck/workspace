package cl.curso.java.builder;

public class Ruedas {

	private int diametro;
	
	
	public Ruedas() {
		// TODO Auto-generated constructor stub
	}


	public Ruedas(int diametro) {
		super();
		this.diametro = diametro;
	}


	public int getDiametro() {
		return diametro;
	}


	public void setDiametro(int diametro) {
		this.diametro = diametro;
	}
	
}
