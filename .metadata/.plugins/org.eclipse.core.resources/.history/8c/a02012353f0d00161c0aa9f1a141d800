
public class Ejercicio6_1 {

	public static void main(String[] args) {
		
		int costo=0;
		int total=0;
		System.out.print("costo:");
		for (int i = 0; i < 20; i++) {
			costo=costo+10;
			System.out.print(costo+" ");
			total=total+costo;
		}
		System.out.println();
		System.out.println("total a pagar es:"+total);
	}

}
