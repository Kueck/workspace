package cl.curso.java.guias.guia11_1.ejercicio1;

public class ProgramaCuentaBancaria {

	public static void main(String[] args) {

		CuentaBancaria cuenta= new CuentaBancaria(50000, 1234);
		
		RetirarDineroThread rtd1=new RetirarDineroThread(cuenta);
		RetirarDineroThread rtd2=new RetirarDineroThread(cuenta);
		RetirarDineroThread rtd3=new RetirarDineroThread(cuenta);
		DepositarDineroThread dtd1=new DepositarDineroThread(cuenta);
		DepositarDineroThread dtd2=new DepositarDineroThread(cuenta);
		DepositarDineroThread dtd3=new DepositarDineroThread(cuenta);
		
		rtd1.start();
		rtd2.start();
		rtd3.start();
		dtd1.start();
		dtd2.start();
		dtd3.start();
		
	}

}
