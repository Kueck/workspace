package guia8.ejercicio2;

public class ProgramaFecha {

	
	public static void main(String[] args)
	{
		
		Fecha f1=new Fecha(100,1,400);
		if(f1.isBisiesto())
			System.out.println("año bisiesto");		
		if(f1.validarDia())
			System.out.println("dia ok");
		if(f1.validarMes())
			System.out.println("mes ok");
		
		System.out.println(f1.getNombreMes());
		
		
	}
	
}
